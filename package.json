{
  "private": true,
  "version": "2.0.0-beta.18",
  "scripts": {
    "start": "NODE_ENV=development babel-node docgen/start.js & npm run storybook & wait",
    "build": "NODE_ENV=production ./scripts/build.sh",
    "docs:build": "DOCS_DIST=docs/react babel-node docgen/build.js && npm run build-storybook",
    "docs:update": "./scripts/docs:update.sh",
    "release": "NODE_ENV=production ./scripts/release.sh",
    "lint": "if [ \"$CI\" = \"true\" ]; then eslint .; else eslint . --cache; fi",
    "lint:fix": "eslint . --fix",
    "doctoc": "doctoc --maxlevel 1 README.md CONTRIBUTING.md",
    "test": "jest && npm run lint && NODE_ENV=production DOCS_MOUNT_POINT=/instantsearch.js/react/ DOCS_DIST=docs/react/ npm run docs:build && NODE_ENV=production npm run build",
    "dev": "jest --watch --bail",
    "coverage": "jest --coverage",
    "storybook": "NODE_ENV=development start-storybook -p 6006 -c ./storybook -s ./storybook/public",
    "build-storybook": "NODE_ENV=production build-storybook -s ./storybook/public -o docs/react/storybook -c ./storybook"
  },
