    "eslint": "3.16.1",
    "eslint-config-algolia": "6.0.1",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jasmine": "2.2.0",
    "eslint-plugin-react": "6.10.0",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "expose-loader": "0.7.3",
    "express": "4.14.1",
    "gh-pages": "1.0.0",
    "http-server": "0.9.0",
    "jsdoc-to-markdown": "1.3.7",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "json": "9.0.6",
    "mkdirp": "0.5.1",
    "mocha": "3.2.0",
    "mversion": "1.10.1",
    "node-sass": "4.5.3",
    "postcss-cli": "2.6.0",
    "preact": "7.2.0",
    "preact-compat": "3.13.1",
    "pretty-bytes-cli": "2.0.0",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.4.2",
    "rimraf": "2.6.2",
    "sass-lint": "1.10.2",
    "semver": "5.3.0",
    "sinon": "2.0.0-pre",
    "surge": "0.18.0",
    "uglify-js": "2.8.0",
    "watch": "1.0.2",
    "wdio-dot-reporter": "0.0.7",
    "wdio-mocha-framework": "0.5.8",
    "wdio-sauce-service": "0.3.0",
    "webdriverio": "4.6.2",
    "webpack": "1.14.0",
