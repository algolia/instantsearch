  "scripts": {
    "boot": "./scripts/boot.sh",
    "upgrade-deps": "./scripts/upgrade-deps.sh",
    "start": "NODE_ENV=development babel-node docgen/start.js & yarn storybook & wait",
    "build": "NODE_ENV=production ./scripts/build.sh",
    "docs:build": "babel-node docgen/build.js && yarn build-storybook",
    "docs:update": "./scripts/docs:update.sh",
    "release": "NODE_ENV=production ./scripts/release.sh",
    "lint": "if [ \"$CI\" = \"true\" ]; then eslint .; else eslint . --cache; fi",
    "lint:fix": "eslint . --fix",
    "doctoc": "doctoc --maxlevel 1 README.md",
    "test": "jest && yarn lint && NODE_ENV=production yarn docs:build && NODE_ENV=production yarn build",
    "dev": "jest --watch --bail",
    "coverage": "jest --coverage",
    "storybook": "NODE_ENV=development start-storybook -p 6006 -c ./storybook -s ./storybook/public",
    "build-storybook": "NODE_ENV=production build-storybook -s ./storybook/public -o docs-production/react/storybook -c ./storybook"
  },
