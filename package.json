    "jsdoc-to-markdown": "^1.3.7",
    "json": "^9.0.6",
    "mkdirp": "^0.5.1",
    "mversion": "^1.10.1",
    "node-sass": "^4.5.0",
    "postcss-cli": "^2.6.0",
