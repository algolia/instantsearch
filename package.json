    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.23.0",
    "babel-template": "6.23.0",
    "babel-types": "6.23.0",
    "cheerio": "0.22.0",
    "collect-json": "1.0.8",
    "compression": "1.7.1",
