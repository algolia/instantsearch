{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions", "ie >= 9"]
            }
          }
        ],
        "react",
        "stage-2"
      ]
    },
    "production": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions", "ie >= 9"]
            }
          }
        ],
        "react",
        "stage-2"
      ]
    },
    "umd": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": ["last 2 versions", "ie >= 9"]
            }
          }
        ],
        "react",
        "stage-2"
      ]
    },
    "es": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": ["last 2 versions", "ie >= 9"]
            }
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": [
        [
          "inline-replace-variables",
          {
            "__DEV__": {
              "type": "node",
              "replacement": "process.env.NODE_ENV === 'development'"
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": ["rewire"]
    }
  }
}
