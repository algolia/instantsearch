{
  "presets": ["latest", "react", "stage-2"],
  "plugins": [
    ["transform-define", {
      "__DOC__": "no"
    }],
  ],
  "env": {
    "test": {
      "plugins": [
        ["babel-plugin-transform-inline-localize-css-import", {
          "localFormat": "$token"
        }]
      ]
    },
    "production": {
      "plugins": [
        ["babel-plugin-transform-inline-localize-css-import", {
          "localFormat": "ais-$cssFilename__$token"
        }]
      ]
    },
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        "react-hot-loader/babel"
      ]
    }
  }
}
