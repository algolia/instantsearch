{
  "name": "instantsearch-react",
  "private": true,
  "scripts": {
    "examples": "webpack-dev-server --config webpack.examples.config.babel.js --hot --content-base examples/static",
    "lint": "eslint \"**/*.js\"",
    "test": "jest --bail --no-cache",
    "test:watch": "jest --watch",
    "test:update-snapshots": "jest -u"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.0.1",
    "jest-cli": "^13.2.3",
    "react-hot-loader": "^1.3.0",
    "react-svg-loader": "^1.0.1",
    "redux-logger": "^2.6.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "algoliasearch": "^3.16.0",
    "algoliasearch-helper": "^2.11.0",
    "babel-jest": "^13.2.2",
    "react": "^15.2.0",
    "react-addons-shallow-compare": "^15.2.0",
    "react-algoliasearch-helper": "^1.0.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-themeable": "^1.1.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "jest": {
    "testRegex": "\\.test\\.js$",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/react-dom/",
      "node_modules/enzyme/",
      "node_modules/lodash/"
    ],
    "setupFiles": [
      "src/silentWarnings.js"
    ]
  }
}
