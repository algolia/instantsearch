{
  "name": "instantsearch-react",
  "private": true,
  "scripts": {
    "start": "./scripts/start.sh",
    "examples": "(cd examples && webpack-dev-server --config webpack.config.babel.js --hot --content-base static --history-api-fallback)",
    "lint": "eslint \"**/*.js\" --ignore-path ../../.gitignore",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:update-snapshots": "jest -u"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "enzyme": "^2.4.1",
    "eslint-config-algolia": "^5.3.1",
    "jest-cli": "^14.1.0",
    "mockdate": "^1.0.4",
    "react-test-renderer": "^15.3.1",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "algoliasearch": "^3.16.0",
    "algoliasearch-helper": "^2.13.0",
    "history": "^2.1.2",
    "lodash": "^4.14.1",
    "qs": "^6.2.1",
    "react": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-themeable": "^1.1.0"
  },
  "jest": {
    "testRegex": "\\.test\\.js$",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/react-dom/",
      "node_modules/react-test-renderer/",
      "node_modules/enzyme/",
      "node_modules/lodash/",
      "node_modules/react-themeable/",
      "node_modules/mockdate/",
      "src/core/propTypes"
    ],
    "globals": {
      "__DOC__": false
    }
  }
}
