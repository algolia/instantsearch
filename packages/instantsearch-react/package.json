{
  "name": "instantsearch-react",
  "private": true,
  "scripts": {
    "start": "./scripts/start.sh",
    "examples": "(cd examples && webpack-dev-server --config webpack.config.babel.js --hot --content-base static --history-api-fallback)",
    "lint": "eslint \"**/*.js\" --ignore-path ../../.gitignore",
    "test": "jest --bail --no-cache",
    "test:watch": "jest --watch",
    "test:update-snapshots": "jest -u"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-jest": "^13.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.0.1",
    "jest-cli": "^13.2.3",
    "mockdate": "^1.0.4",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "algoliasearch": "^3.16.0",
    "algoliasearch-helper": "^2.11.1",
    "history": "^2.1.2",
    "rc-slider": "^3.8.2",
    "react": "^15.2.0",
    "react-addons-shallow-compare": "^15.2.0",
    "react-algoliasearch-helper": "^1.2.1",
    "react-dom": "^15.2.0",
    "react-themeable": "^1.1.0"
  },
  "jest": {
    "testRegex": "\\.test\\.js$",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/react-dom/",
      "node_modules/enzyme/",
      "node_modules/lodash/",
      "node_modules/react-themeable/",
      "node_modules/mockdate/"
    ],
    "setupFiles": [
      "src/silentWarnings.js"
    ]
  }
}
