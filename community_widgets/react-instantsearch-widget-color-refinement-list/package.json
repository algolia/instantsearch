    "lint": "eslint --ext .js,.ts,.tsx .",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:types": "tsc",
    "prerelease": "npm run build",
    "release": "standard-version"
